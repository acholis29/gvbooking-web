generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model MSExcursion {
  Country        String?  @db.NVarChar(50)
  State          String?  @db.NVarChar(500)
  code_exc       String?  @db.NVarChar(50)
  Idx_excursion  String   @id(map: "PK_MSExcursion") @db.NVarChar(50)
  Name_excursion String?  @db.NVarChar(500)
  Duration_Type  String?  @db.NVarChar(500)
  Holiday_Type   String?  @db.NVarChar(500)
  PriceFrom      Float?
  Descr          String?  @db.NVarChar(2500)
  StsDel         Boolean?
  StsRecom       Boolean?
  idx_comp       String?  @db.NVarChar(50)
}
