generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model tour {
  id        BigInt  @id
  nama_tour String? @db.VarChar(255)
  harga     Int?
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}
